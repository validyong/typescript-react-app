,\r\n    company: string,\r\n    price: number,\r\n    genre_code: number\r\n}\r\n\r\nexport interface IFormState {\r\n    [key: string]: any;\r\n    values: IValues[];\r\n    submitSuccess: boolean;\r\n    loading: boolean;\r\n}\r\n\r\nclass Create extends React.Component<RouteComponentProps, IFormState> {\r\n    constructor(props: RouteComponentProps) {\r\n        super(props);\r\n        this.state = {\r\n            isbn: '',\r\n            book_name: '',\r\n            company: '',\r\n            price: 0,\r\n            genre_code: 0,\r\n            values: [],\r\n            loading: false,\r\n            submitSuccess: false,\r\n        }\r\n    }\r\n\r\n    private processFormSubmission = (e: React.FormEvent<HTMLFormElement>): void => {\r\n        e.preventDefault();\r\n        this.setState({ loading: true });\r\n        const formData = {\r\n            isbn: this.state.isbn,\r\n            book_name: this.state.book_name,\r\n            company: this.state.company,\r\n            price: this.state.price,\r\n            genre_code: this.state.genre_code,\r\n        }\r\n        this.setState({ submitSuccess: true, values: [...this.state.values, formData], loading: false });\r\n        axios.post(`http://localhost:5000/book`, formData).then(data => [\r\n            setTimeout(() => {\r\n                this.props.history.push('/');\r\n            }, 1500)\r\n        ]);\r\n    }\r\n\r\n    private handleInputChanges = (e: React.FormEvent<HTMLInputElement>) => {\r\n        e.preventDefault();\r\n        this.setState({\r\n            [e.currentTarget.name]: e.currentTarget.value,\r\n        })\r\n    }\r\n}\r\n\r\nexport default withRouter(Create)"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"a40a485b680fd0e6a8c18dd5ca5e16349f750858","contentHash":"b87507383e9ddc01f2289470ad463c797d8fcd6ceb3488cc6145c59844767579"}}